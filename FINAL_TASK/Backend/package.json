{
  "name": "tsoastarterkit",
  "version": "1.0.0",
  "description": "A Stater Kit with User Management",
  "main": "dist/src/server.js",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "postinstall": "tsoa spec-and-routes",
    "build": "tsoa spec-and-routes && tsc",
    "start": "export NODE_ENV=test || set NODE_ENV=test&& npm run start-n",
    "test": "export NODE_ENV=test || set NODE_ENV=test&& jest",
    "start-n": "tsoa spec-and-routes && tsc && node dist/src/server.js",
    "build-n-test": "tsoa spec-and-routes && tsc && jest",
    "run-prod": "node dist/src/server.js",
    "dev": "export NODE_ENV=test || set NODE_ENV=test&& nodemon"
  },
  "author": "Kurian Thomas",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.0",
    "config": "^3.3.7",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.10.0",
    "mongoose": "^6.6.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.5",
    "swagger-ui-express": "^4.3.0",
    "tsoa": "^4.1.3",
    "typescript-ioc": "^3.2.2",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.6.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/config": "^0.0.41",
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.25",
    "@types/nodemailer": "^6.4.4",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "jest": "^28.1.3",
    "nodemon": "^2.0.15",
    "supertest": "^6.3.0",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.4"
  }
}
